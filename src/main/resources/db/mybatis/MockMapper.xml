<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.feiyongjing.MockMapper">
    <insert id="insertNews" parameterType="com.github.feiyongjing.News">
        insert into NEWS (URL,TITLE,CONTENT,CREATED_AT,MODIFIED_AT)
        values (#{url},#{title},#{content},#{createdAt},#{modifiedAt})
    </insert>
    <select id="selectNews" resultType="com.github.feiyongjing.News">
     select id, url, title, content, created_at, modified_at from NEWS LIMIT 200
     </select>
</mapper>